<!doctype html>
<html lang="de">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ renderData.title or title or metadata.title }}</title>
    <meta name="description" content="{{ renderData.description or description or metadata.description }}">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Fira+Code:wght@300..500&family=IM+Fell+French+Canon:ital@1&family=Noto+Serif+Display:wght@100..500&display=swap" rel="stylesheet">
    <link href="{{ 'css/style.css' | url }}" rel="stylesheet" type="text/css">
  </head>
  <body class="site">
    {{ content | safe }}
  </body>
</html>
